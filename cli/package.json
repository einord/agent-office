{
  "name": "agent-office",
  "version": "0.1.5",
  "description": "Real-time monitoring CLI for Claude AI agents",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent-office": "dist/index.js"
  },
  "scripts": {
    "dev": "tsc && TERM=xterm-256color node dist/index.js --server-url http://localhost:3100 --api-key abc123 || true",
    "build": "tsc",
    "start": "TERM=xterm-256color node dist/index.js || true"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "claude",
    "ai",
    "agent",
    "monitor",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/einord/agent-office.git",
    "directory": "cli"
  },
  "engines": {
    "node": ">=18"
  },
  "pkg": {
    "assets": [
      "dist/**/*"
    ],
    "outputPath": "bin"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.27",
    "@types/node": "^20.0.0",
    "nodemon": "^3.1.11",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  }
}
